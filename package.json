{
  "name": "@csas-read-app-metadata/action",
  "version": "2.3.0",
  "description": "Read ArgoCD Applications metadata",
  "author": "Ceska sporitelna, a.s.",
  "scripts": {
    "prebuild": "rimraf lib",
    "build": "tsc --project tsconfig.build.json",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint .",
    "fix": "prettier --write **/*.ts && eslint . --fix",
    "package": "ncc build src/index.ts -o dist",
    "test": "jest --coverage",
    "all": "npm run lint && npm run format-check && npm run package && npm test",
    "prepack": "npm run build"
  },
  "keywords": [
    "actions",
    "argocd",
    "metadata"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/csas-actions/read-app-metadata.git"
  },
  "files": [
    "lib/src"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@types/glob": "^8.0.0",
    "glob": "^10.2.7",
    "yaml": "^2.4.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^16.18.91",
    "@typescript-eslint/parser": "^7.4.0",
    "@vercel/ncc": "^0.38.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-github": "^4.10.2",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "jest": "^29.7.0",
    "nock": "^13.5.4",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.3"
  }
}
